procedure spatial_p_proc begin
   if (getQuest(GVAR_VAULT27_GENERATOR) >= qEnd) then begin
       if (source_obj == dude_obj) then begin
           play_sfx("ELV1_2");
           gfade_out(ONE_GAME_SECOND);
